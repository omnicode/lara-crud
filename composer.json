{
	"name": "omnicode/lara-crud",
	"description": "Ready CRUD actions",
	"keywords": ["laravel", "bootstrapping", "crud"],
	"license": "MIT",
	"authors": [
		{
			"name": "Omnicode"
		}
	],
	"require": {
		"laravel/framework": "5.6.*",
		"cakephp/utility": "~3.3",
		"laracasts/flash": "~2.0 || ~3.0"
	},
	"require-dev": {
		"mockery/mockery": "~1.0",
		"phpunit/phpunit": "~7.0"
	},
	"suggest": {
		"omnicode/lara-view-composer": "Useful view composers to populate dropdown lists",
		"omnicode/lara-service": "Lara crud integrated with LaraService",
		"omnicode/lara-repo": "Lara crud integrated with LaraRepo",
		"omnicode/lara-model": "Lara crud integrated with LaraModel",
		"omnicode/lara-view": "Lara crud integrated with LaraView"
	},
	"autoload": {
		"psr-4": {
			"LaraCrud\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	}
}
